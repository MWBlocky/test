{
  "version": 2,
  "builds": [
    {
      "src": "generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "X-Requested-With,Content-Type,Accept"
      }
    }
  ],
  "crons": [
    {
      "path": "/",
      "schedule": "20 11 * * 4"
    }
  ],
  "env": {
    "CRON_SECRET": "secrbuibet"
  }
}